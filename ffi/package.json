{
  "name": "bitpermjs",
  "version": "0.1.0",
  "private": true,
  "description": "Javascript module generated using Rust FFI via Neon.",
  "main": "build/index.node",
  "types": "build/index.d.ts",
  "scripts": {
    "prebuild": "rimraf ./build",
    "build": "cargo-cp-artifact -nc types/index.node -- cargo build --message-format=json-render-diagnostics",
    "build-debug": "npm run build --",
    "build-release": "npm run build -- --release",
    "build-ts": "tsc -p tsconfig.json && copyfiles -f types/index.node build",
    "postbuild": "npm run build-ts",
    "install": "npm run build-release",
    "test": "cargo test"
  },
  "author": "Alexandra Belluscio",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/jest": "^29.5.12",
    "cargo-cp-artifact": "^0.1",
    "copyfiles": "^2.4.1",
    "jest": "^29.7.0",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.4",
    "typescript": "^5.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qtapostate/bitperm.git"
  },
  "bugs": {
    "url": "https://github.com/qtapostate/bitperm/issues"
  },
  "homepage": "https://github.com/qtapostate/bitperm#readme"
}
